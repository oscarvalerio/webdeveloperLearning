<% layout('/layouts/boilerplate') %>
<div class="row">
  <div class="col-6 offset-3">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading">Upss, something failed in the process!</h4>
      <h5 class="alert-heading"> <%=err.message%>  </h5>
      <p>Please check the URL you tried and give it another shot.</p>
      <hr>
      <p>You can navigate to homepage from the navigation bar.</p>
      <hr>
      <p> <%= err.stack %>  </p>
    </div>
  </div>
</div>

